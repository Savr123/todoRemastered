{
  "_version": "1.12.0",
  "sap.app": {
  	"id": "todo",
  	"type": "application",
  	"i18n": "i18n/i18n.properties",
  	"title": "{{appTitle}}",
  	"description": "{{appDescription}}",
  	"applicationVersion": {
  	  "version": "1.0.0"
  	},
    "dataSources": {
      "invoiceRemote": {
        "uri": "https://services.odata.org/V2/Northwind/Northwind.svc/",
        "type": "oData",
        "settings": {
          "oDataVersion": "2.0"
        }
      }
    }
  },
  "sap.ui": {
	"technology": "UI5",
	"deviceTypes": {
		"desktop": true,
		"tablet": true,
		"phone": true
	}
  },
  "sap.ui5": {
  	"rootView": {
  		"viewName": "todo.view.App",
  		"type": "XML",
  		"async": true,
  		"id": "app"
  	},
  	"dependencies": {
  	  "minUI5Version": "1.60",
  	  "libs": {
  		"sap.m": {}
  	  }
  	},
  	"models": {
  	  "i18n": {
    		"type": "sap.ui.model.resource.ResourceModel",
    		"settings": {
    		  "bundleName": "todo.i18n.i18n"
    		}
  	  }
  	},
  	"routing": {
  	  "config": {
    		"routerClass": "sap.m.routing.Router",
    		"viewType": "XML",
    		"viewPath": "todo.view",
    		"controlId": "app",
    		"controlAggregation": "pages",
        "transition":"slide",
    		"async": true
  	  },
  	  "routes": [
  		{
  		  "pattern": "",
  		  "name": "overview",
  		  "target": "overview"
  		},
  		{
        "pattern": "detail/{invoicePath}",
  		  "name": "detail",
  		  "target": "detail"
  		}
  	  ],
  	  "targets": {
    		"overview": {
    		  "viewId": "overview",
    		  "viewName": "Overview"
    		},
    		"detail": {
    		  "viewId": "detail",
    		  "viewName": "Detail"
    		}
  	  }
  	},
    "resources":{
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    }
  }
}
